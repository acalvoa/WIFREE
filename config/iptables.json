			"-X facebooklist -t mangle",
			"-X facebookip -t mangle",
			"-X servicios -t mangle",
			"-N internet -t mangle",
			"-N facebooklist -t mangle",
			"-N facebookip -t mangle",
			"-N servicios -t mangle",
			"-t mangle -A PREROUTING -j internet",
			"-t mangle -A internet -s $%IPCRM% -j MARK --set-mark 12",
			"-t mangle -A internet -m mark --mark 12 -j RETURN",
