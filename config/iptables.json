			"-t filter -A INPUT -p tcp --dport 443 -j ACCEPT",
			"-t filter -A INPUT -j DROP",
			"-t filter -A FORWARD -m mark --mark 13 -j ACCEPT",
			"-t filter -A FORWARD -m mark --mark 10 -j DROP",
			"-t filter -A FORWARD -i $%IN% -o $%OUT% -m state --state ESTABLISHED,RELATED -j ACCEPT",
			"-t filter -A FORWARD -i $%OUT% -o $%IN% -j ACCEPT",
			"-t filter -A FORWARD -j DROP",
			"-t nat -A POSTROUTING -o $%IN% -j MASQUERADE"
